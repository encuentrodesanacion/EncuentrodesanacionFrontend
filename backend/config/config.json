{
  "development": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "port": 5432,
    "seederStorage": "sequelize",
    "seederStorageTableName": "sequelize_data"
  },
  "test": {
    "dialect": "sqlite",
    "storage": ":memory:"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "port": 5432,
    "seederStorage": "sequelize",
    "seederStorageTableName": "sequelize_data",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
